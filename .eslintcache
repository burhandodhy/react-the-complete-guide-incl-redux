 0, cheese: 0, salad: 0 });\n  const [price, setPrice] = useState(0);\n  const [purchasable, setPurchasable] = useState(false);\n  const [isPurchasing, setIsPurchasing] = useState(false);\n\n  const addIngredientHandler = (key) => {\n    let oldVal = ingredients[key];\n    let newVal = oldVal + 1;\n    let newIngredients = { ...ingredients };\n    newIngredients[key] = newVal;\n    setIngredients(newIngredients);\n\n    let oldPrice = price;\n    let newPrice = oldPrice + INGREDIENTS_PRICE[key];\n    setPrice(newPrice);\n    updatePurchaseState(newIngredients);\n  };\n\n  const removeIngredientHandler = (key) => {\n    let oldVal = ingredients[key];\n    if (oldVal <= 0) {\n      return;\n    }\n    let newVal = oldVal - 1;\n    let newIngredients = { ...ingredients };\n    newIngredients[key] = newVal;\n    setIngredients(newIngredients);\n\n    let oldPrice = price;\n    let newPrice = oldPrice - INGREDIENTS_PRICE[key];\n    setPrice(newPrice);\n    updatePurchaseState(newIngredients);\n  };\n\n  const disabledInfo = Object.fromEntries(Object.entries(ingredients).map(([k, v]) => [k, v <= 0]));\n\n  const updatePurchaseState = (ingredients) => {\n    const sum = Object.values(ingredients).reduce((total, currentValue) => {\n      return total + currentValue;\n    }, 0);\n    setPurchasable(sum > 0);\n  };\n\n  const isPurchasingHandler = () => {\n    setIsPurchasing(true);\n  };\n\n  const cancelPurchaseHandler = () => {\n    setIsPurchasing(false);\n  };\n\n  const continuePurchaseHandler = () => {\n    console.log(\"Success\");\n  };\n\n  return (\n    <Fragment>\n      <Modal show={isPurchasing} modelClose={cancelPurchaseHandler}>\n        <OrderSummary\n          ingredients={ingredients}\n          cancelHandler={props.cancelPurchaseHandler}\n          continueHandler={props.continuePurchaseHandler}\n          price={price}\n        />\n      </Modal>\n      <Burger ingredients={ingredients} />\n      <BuildControls\n        addIngredient={addIngredientHandler}\n        removeIngredient={removeIngredientHandler}\n        disabledInfo={disabledInfo}\n        price={price}\n        purchasable={purchasable}\n        purchasing={isPurchasingHandler}\n      />\n    </Fragment>\n  );\n};\n\nexport default BurgerBuilder;\n","/Users/burhannasir/burhan/learning/react-the-complete-guide-incl-redux/react-the-complete-guide-incl-redux/src/components/Burger/Burger.js",[],"/Users/burhannasir/burhan/learning/react-the-complete-guide-incl-redux/react-the-complete-guide-incl-redux/src/components/Burger/BurgerIngredients/BurgerIngredients.js",[],"/Users/burhannasir/burhan/learning/react-the-complete-guide-incl-redux/react-the-complete-guide-incl-redux/src/components/BuildControls/BuildControls.js",[],"/Users/burhannasir/burhan/learning/react-the-complete-guide-incl-redux/react-the-complete-guide-incl-redux/src/components/BuildControls/BuildControl/BuildControl.js",[],"/Users/burhannasir/burhan/learning/react-the-complete-guide-incl-redux/react-the-complete-guide-incl-redux/src/components/UI/Modal/Modal.js",[],"/Users/burhannasir/burhan/learning/react-the-complete-guide-incl-redux/react-the-complete-guide-incl-redux/src/components/OrderSummary/OrderSummary.js",[],"/Users/burhannasir/burhan/learning/react-the-complete-guide-incl-redux/react-the-complete-guide-incl-redux/src/components/UI/Backdrop/Backdrop.js",["84"],"import React from 'react'\n\nimport classes from './Backdrop.module.css'\n\nconst Backdrop = (props) => (props.show == true ? <div className={classes.Backdrop} onClick={props.clickHandler}></div> : null);\n\nexport  default Backdrop","/Users/burhannasir/burhan/learning/react-the-complete-guide-incl-redux/react-the-complete-guide-incl-redux/src/components/UI/Button/Button.js",[],"/Users/burhannasir/burhan/learning/react-the-complete-guide-incl-redux/react-the-complete-guide-incl-redux/src/components/Navigation/Toolbar/Toolbar.js",[],"/Users/burhannasir/burhan/learning/react-the-complete-guide-incl-redux/react-the-complete-guide-